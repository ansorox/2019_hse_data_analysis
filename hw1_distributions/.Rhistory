f <- function(x){
return (dlnorm(x, mean = 3, sd = 0.5))
}
integrate(f, 25, 50)
data <- data_frame(p = seq(0.1, 1, by = 0.1),
val = qlnorm(p, mean = 3, sd = 0.5))
integrate(f, data$val[1],data$val[9])
